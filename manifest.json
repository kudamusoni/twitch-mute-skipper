{
    "manifest_version": 3,
    "name": "Twitch Skippa",
    "version": "1.0.0",
    "icons": {
        "16": "public/sword.png",
        "32": "public/sword.png",
        "48": "public/sword.png",
        "128": "public/sword.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab","scripting","tabs"],
    "content_scripts": [{
        "js": ["scripts/content.js"],
        "matches": [
            "https://www.twitch.tv/*",
            "https://www.twitch.tv/*"
        ]
    }]
}