{
    "manifest_version": 3,
    "name": "VOD Skippa",
    "version": "1.0.0",
    "icons": {
        "16": "public/sword.png",
        "32": "public/sword.png",
        "48": "public/sword.png",
        "128": "public/sword.png"
    },
    "permissions": ["activeTab","scripting","tabs"],
    "content_scripts": [{
        "js": ["scripts/content.js"],
        "matches": [
            "https://www.twitch.tv/videos/*"
        ]
    }]
}